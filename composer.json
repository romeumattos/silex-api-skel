{
  "name" : "romeumattos/twitter-like",
  "description" : "Twitter Like REST Application",
  "license" : "MIT",
  "homepage" : "https://github.com/romeumattos/twitter-like.git",
  "type": "project",
  "minimum-stability": "dev",
  "autoload" : {
    "psr-0" : {
      "Palma\\": "vendor/palma/DoctrineORMServiceProvider/lib/",
      "JMS\\": "vendor/jms/serializer/src/"
    },
    "psr-4" : {
      "Common\\": "src/Common/src/",
      "Message\\": "src/Message/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Common\\Tests\\": "src/Common/tests/",
      "Doctrine\\Tests\\": "vendor/doctrine/orm/tests/Doctrine/Tests/",
      "Message\\Tests\\": "src/Message/tests/"
    }
  },
  "authors" : [
    {
      "name" : "Thiago Paes",
      "email" : "mrprompt@gmail.com",
      "homepage" : "http://www.mrprompt.com.br",
      "role" : "Developer"
    },
    {
      "name" : "Romeu Mattos",
      "email" : "romeu.smattos@gmail.com",
      "homepage" : "http://www.romeumattos.com.br",
      "role" : "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-pdo": "*",
    "ext-pdo_sqlite": "*",
    "ext-json": "*",
    "ext-intl": "*",
    "ext-xsl": "*",
    "doctrine/migrations": "1.0.*@dev",
    "doctrine/orm": "~2.5",
    "deralex/yaml-config-service-provider": "~1.0",
    "jms/serializer": "*",
    "monolog/monolog": "~1.11",
    "mrprompt/silex-di-builder": "dev-master",
    "mrprompt/silex-cors-provider": "dev-master",
    "mrprompt/silex-router-provider": "dev-master",
    "palma/DoctrineORMServiceProvider": "dev-master",
    "respect/validation": "~1.0",
    "silex/silex": "~1.3",
    "symfony/yaml": "~2.6",
    "knplabs/console-service-provider": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.2.10",
    "phing/phing": "~2.8",
    "squizlabs/php_codesniffer": "2.*",
    "mockery/mockery": "1.0.x-dev",
    "symfony/browser-kit": "~2.5",
    "phploc/phploc": "*",
    "pdepend/pdepend" : "2.0.6",
    "phpmd/phpmd" : "@stable",
    "sebastian/phpcpd": "*",
    "theseer/phpdox": "dev-master",
    "friendsofphp/php-cs-fixer": "*",
    "useallfive/doctrine-data-fixtures-command": "dev-master",
    "codeclimate/php-test-reporter": "dev-master"
  }
}
